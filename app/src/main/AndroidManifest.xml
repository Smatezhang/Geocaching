<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.zhuoxin.zhang.geocaching">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".base.GeocachingApplication"
        android:allowBackup="true"
        android:icon="@mipmap/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".user.register.RegisterActivity">
        </activity>
        <activity android:name=".user.login.LoginActivity">
        </activity>
        <activity android:name=".map.HomeActivity">
        </activity>
    </application>

</manifest>